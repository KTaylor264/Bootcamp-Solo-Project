function checkStages(boolStages, stages)
{	
	console.log(boolStages);
	console.log(stages);

	for (var i = 0; i < boolStages.length; i++)
	{
		if (boolStages[i] == true)
		{
			var check = document.getElementsByName(stages[i].stageName);
			check.checked = true;
		}
		else
		{
			var check = document.getElementsByName(stages[i].stageName);
			check.checked = false;
		}
	}
}
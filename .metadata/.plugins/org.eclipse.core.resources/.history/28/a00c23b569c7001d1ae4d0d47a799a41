function checkStages(boolStages)
{	
	console.log(boolStages);

	for (var i = 0; i < boolStages.length; i++)
	{
		if (boolStages[i] == true)
		{
			switch (i)
			{
				case 0:
				{
					console.log(check);
					var check = document.getElementsByName("Baby");
					check.checked = true;
					console.log(check);
					console.log("baby is checked");
				}
				break;
				case 1:
				{
					var check = document.getElementsByName("In-Training");
					check.checked = true;
				}
				break;
				case 2:
				{
					var check = document.getElementsByName("Rookie");
					check.checked = true;
				}
				break;
				case 3:
				{
					var check = document.getElementsByName("Champion");
					check.checked = true;
				}
				break;
				case 4:
				{
					var check = document.getElementsByName("Ultimate");
					check.checked = true;
				}
				break;
				case 5:
				{
					var check = document.getElementsByName("Mega");
					check.checked = true;
				}
				break;
				case 6:
				{
					var check = document.getElementsByName("Ultra");
					check.checked = true;
				}
				break;
				case 7:
				{
					var check = document.getElementsByName("Armor");
					check.checked = true;
				}
				break;
				case 8:
				{
					var check = document.getElementsByName("None");
					check.checked = true;
				}
				break;
			}
		}
		else
		{
			switch (i)
			{
				case 0:
				{
					console.log(check);
					var check = document.getElementsByName("Baby");
					check.checked = false;
					console.log(check);
					console.log("baby is not checked");
				}
				break;
				case 1:
				{
					var check = document.getElementsByName("In-Training");
					check.checked = false;
				}
				break;
				case 2:
				{
					var check = document.getElementsByName("Rookie");
					check.checked = false;
				}
				break;
				case 3:
				{
					var check = document.getElementsByName("Champion");
					check.checked = false;
				}
				break;
				case 4:
				{
					var check = document.getElementsByName("Ultimate");
					check.checked = false;
				}
				break;
				case 5:
				{
					var check = document.getElementsByName("Mega");
					check.checked = false;
				}
				break;
				case 6:
				{
					var check = document.getElementsByName("Ultra");
					check.checked = false;
				}
				break;
				case 7:
				{
					var check = document.getElementsByName("Armor");
					check.checked = false;
				}
				break;
				case 8:
				{
					var check = document.getElementsByName("None");
					check.checked = false;
				}
				break;
			}
		}
	}
}